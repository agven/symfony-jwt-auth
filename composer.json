{
    "name": "agven/symfony-jwt-auth",
    "type": "symfony-bundle",
    "description": "JWT Authentication Bundle for Symfony REST API",
    "keywords":    [
        "symfony",
        "authentication",
        "refresh token",
        "jwt token",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/agven/symfony-jwt-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrei Bashko",
            "email": "a.g.bashko@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Agven\\JWTAuthBundle\\": "src/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "firebase/php-jwt": "~5.0",
        "symfony/framework-bundle": "4.3.*",
        "symfony/security-guard": "4.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1"
    },
    "config": {
        "sort-packages": true
    }
}
